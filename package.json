{
  "name": "nextbot",
  "version": "1.3.2",
  "description": "Cross-platform FSM constructor for chatbots",
  "main": "dist/index.js",
  "scripts": {
    "start": "LOGLEVEL=err node ./dist/index.js",
    "dev": "LOGLEVEL=err nodemon ./dist/index.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextb1t/nextbot-core.git"
  },
  "keywords": [
    "nextbot",
    "typescript",
    "bot",
    "chatbot",
    "bot framework",
    "fsm"
  ],
  "author": "Alena Gilevskaya <alena@gilevskaya.ru>",
  "bugs": {
    "url": "https://github.com/nextb1t/nextb0t-core/issues"
  },
  "homepage": "https://github.com/nextb1t/nextb0t-core#readme",
  "license": "MIT",
  "dependencies": {
    "loglevel": "^1.4.1",
    "typescript.events": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.4",
    "@types/node": "^8.0.17",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "ts-jest": "^20.0.5",
    "typescript": "^2.3.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
